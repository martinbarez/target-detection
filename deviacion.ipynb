   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy\n",
    "\n",
    "def deviacion(X, media_bandas):\n",
    "    Xt = numpy.transpose(X.copy())\n",
    "\n",
    "    #A cada toma le resto la media de su banda\n",
    "    deviacion = Xt-media_bandas\n",
    "    return deviacion"
   ]
  }
